pico-8 cartridge // http://www.pico-8.com
version 8
__lua__


function _init()	
	t = 0

	player = {}
	player.sp = 1
	player.x=10
	player.y=64
	player.dir = 0 --player direction(in degrees)
	player.speed = 0.7
	player.tick=1 --for animations

	zombies = {}
	bullets = {}
end

function _update()
	updatePlayer()
		
	foreach(zombies, updatezombies)
	for b in all(bullets) do
		b.x+= b.velocity
		if b.x < 0 or b.x > 128 or
	 		b.y < 0 or b.y > 128 then
	 		del(bullets,b)
		end
	end
	zombiebulletcollision()
end

function updatePlayer()
	player.tick +=1

	if btn(2) then
		player.y -= player.speed
	end
	if btn(3) then
		player.y += player.speed
	end
	if btn(0) then
		player.x -= player.speed
	end
	if btn(1) then
		player.x += player.speed
	end

	if btnp(4) then
		spawnzombie()
	end

	if btnp(5) then
		shoot()
	end
end

function _draw()
	cls(1)

	spr(player.sp,player.x,player.y,2,2)
	
	for z in all(zombies) do
		spr(z.sp,z.x,z.y,2,2)
	end

	for b in all(bullets) do
		spr(b.sp,b.x,b.y)
	end
end

function updatezombies(z)
	z.tick-=1

	if z.tick<=0 then
			if rnd()>0.2 then
				if player.x<z.x then
					z.x-=1
				end
				if player.x>z.x then
				 z.x+=1
				end
				if player.y<z.y then
				 z.y-=1
				end
				if player.y>z.y then
				 z.y+=1
				end
			else --
		z.tick=rndb(45,60)
		end
	end
end

function rndb(l,h)
	return flr(rnd(h-l)+l)
end

function enemydamage(bullet,zombie) 
	del(bullets,bullet)
	zombie.damaged = true
	zombie.health-=1

	if zombie.health == 0 then
		removezombie(zombie)
	end
end

function removezombie(zombie)
	del(zombies,zombie)
end

function spawnzombie()
	zombie = {}
	zombie.sp = 33
	zombie.x = flr(rnd(120))
	zombie.y = flr(rnd(120))
	zombie.tick = rndb(45,60)
	zombie.health = 2
	zombie.damaged = true

	add(zombies,zombie)
end

function shoot()
	bullet = {}
	bullet.x=player.x
	bullet.y=player.y
	bullet.sp = 0
	bullet.velocity = 4

	add(bullets,bullet)
end

function zombiebulletcollision()
	for bullet in all(bullets) do
		for zombie in all(zombies) do
			if (bullet.x+3<zombie.x+7) and
				bullet.x+5>zombie.x+1 and
				bullet.y+8>zombie.y+4 and
				bullet.y+8>zombie.y then
				enemydamage(bullet,zombie)
			end
		end
	end
end

function rotate(player,r)
	player.dir += r
	if player.dir < 0 then
		player.dir=355
		elseif player.dir > 355 then
			player.dir=0
		end
end




pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--0.0.1
--todo: make debug mode

function _init()	
	t = 0 --game tick

	player = {}
	player.sp = 0 --default sprite
	player.x=10
	player.y=64
	--velocity
	player.dx=0
	player.dy=0

	player.dir = 0 --player direction
	player.speed = 0.8
	player.tick=1 

	--states
	player.walk = false
	player.sprint = false
	player.sneak = false
	player.flip = false
	player.grounded = false

	zombies = {}
	bullets = {}
	grass = {}

	gravity = 0.2
end

function _update()
	updateplayer()
		
	foreach(zombies, updatezombies)
	for b in all(bullets) do
		b.x+= b.velocity
		if b.x < 0 or b.x > 128 or
	 		b.y < 0 or b.y > 128 then
	 		del(bullets,b)
		end
	end
	zombiebulletcollision()
end

function idleAnimation()
	if player.walk == false then
		player.sp = 0
	end
end

function updateplayer()
	idleAnimation()
	--timer for idle animations
	player.tick += 1

	if player.tick == 60 then
		player.tick = 0
	end

	if btn(0) then
		player.x -= player.speed
		player.walk = true
		player.flip = true
		player.sp += 2

		if player.sp > 6 then
			player.sp = 2
		end
	end

	if not btn(0) then
		player.walk = false
	end

	if btn(1) then
		player.x += player.speed
		player.walk = true
		player.flip = false
		player.sp += 2

		if player.sp > 6 then
			player.sp = 2
		end
	end

	if btn(2) then
		player.y -= player.speed
	end

	if btn(3) then
		player.y += player.speed
	end
	--will become attack button
	if btnp(4) then
		spawnzombie()
	end
--reserved for jump button
	if btnp(5) then
		player.y-= 5
		player.sp = 34
	end
end

function _draw()
	cls(1)
	map(0,0,0,0,16,16)

	spr(player.sp,player.x,player.y,2,2,player.flip)
	
	for z in all(zombies) do
		spr(z.sp,z.x,z.y,2,2,z.flip)
	end

	for b in all(bullets) do
		spr(b.sp,b.x,b.y)
	end

	--print(player.tick,35,40,0)
	--print(player.walk,36,8,0)

end

function updatezombies(z)
	z.tick-=1

	if z.tick<=0 then
			if rnd()>0.2 then
				if player.x<z.x then
					z.x-=1
					z.flip = true
				end
				if player.x>z.x then
				 z.x+=1
				 if z.flip == true then
				 z.flip = false
					end
				end
				if player.y<z.y then
				 z.y-=1
				end
				if player.y>z.y then	
				 z.y+=1
				end
			else --
		z.tick=rndb(45,60)
		end
	end
end

function rndb(l,h)
	return flr(rnd(h-l)+l)
end

function enemydamage(bullet,zombie) 
	del(bullets,bullet)
	zombie.damaged = true
	zombie.health-=1
	if zombie.health == 0 then
		removezombie(zombie)
	end
end

function removezombie(zombie)
	del(zombies,zombie)
end

--for development purposes, in first built enemies will spawn randomly
function spawnzombie()
	zombie = {}
	zombie.sp = 64
	zombie.x = flr(rnd(120))
	zombie.y = flr(rnd(120))
	zombie.tick = rndb(45,60)
	zombie.health = 2
	zombie.damaged = false
	zombie.flip=false

	add(zombies,zombie)
end

--Deprecated
function shoot()
	bullet = {}
	bullet.x=player.x
	bullet.y=player.y
	bullet.sp = 0
	bullet.velocity = 4

	add(bullets,bullet)
end

function zombiebulletcollision()
	for bullet in all(bullets) do
		for zombie in all(zombies) do
			if (bullet.x+3<zombie.x+7) and
				bullet.x+5>zombie.x+1 and
				bullet.y+8>zombie.y+4 and
				bullet.y+8>zombie.y then
				enemydamage(bullet,zombie)
			end
		end
	end
end






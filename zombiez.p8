pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function _init()
	-- poke(0x5f2d, 1) --mouse
	
	player = {}
	player.sp = 1
	player.x=0
	player.y=0
	player.d = 0
	player.speed = 2

	zombies = {}
end

function _update()
	if btn(2) then
		player.y -= player.speed
	end
	if btn(3) then
		player.y += player.speed
	end
	if btn(0) then
		player.x -= player.speed
	end
	if btn(1) then
		player.x += player.speed
	end

	if btn(4) then
		spawnzombie()
	end

	foreach(zombies, updateZombies)
end

function _draw()
	cls()
	spr(player.sp,player.x,player.y)

	for z in all(zombies) do
		spr(z.sp,z.x,z.y)
	end

	-- spr(2,stat(32)-1,stat(33)-1) --cursor
end

function updateZombies(z)
	z.tick-=1

	if z.tick<=0 then
	
		--maybe move toward player
		if rnd()>0.2 then
			if player.x<z.x then
				z.x-=1
			end
			if player.x>z.x then
			 z.x+=1
			end
			if player.y<z.y then
			 z.y-=1
			end
			if player.y>z.y then
			 z.y+=1
			end
		else --or move randomly
			-- z.x=rndb(-1,1)
			-- z.y=rndb(-1,1)
		-- end
		--reset move timer
		z.tick=rndb(45,60)
		-- z.x = flr(rnd(120))
		-- z.y = flr(rnd(120))
		end
	end
end

function rndb(l,h)
	return flr(rnd(h-l)+l)
end

function spawnzombie()
	zombie = {}
	zombie.sp = 2
	zombie.x = flr(rnd(120))
	zombie.y = flr(rnd(120))
	zombie.tick = rndb(45,60)

	add(zombies,zombie)
end



